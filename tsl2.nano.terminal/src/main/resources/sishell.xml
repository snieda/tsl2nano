               <method>executeAndGetOutput</method>
               <argNames length="0"/>
               <cmd>echo</cmd>
            </doAction>
            <sequence class="de.tsl2.nano.incubation.terminal.item.selector.XPathSelector" name="xpathselector" multiple="false" sequential="false">
               <xml>bin/sishell.xml</xml>
               <xpath>//@name</xpath>
            </sequence>
         </foreach>
      </container>
      <container name="Html" multiple="false" sequential="false">
         <fileSelector name="source" multiple="false" sequential="false">
