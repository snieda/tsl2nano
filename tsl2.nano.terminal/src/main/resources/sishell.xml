         </input>
         <mainaction name="Permutator">
            <mainClass>de.tsl2.nano.core.secure.Permutator</mainClass>
            <method>main</method>
            <argNames length="4">
               <string>source</string>
               <string>transformer</string>
               <string>swap</string>
               <string>backward</string>
            </argNames>
         </mainaction>
      </container>
      <container name="Xml" multiple="false" sequential="false">
