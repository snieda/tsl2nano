CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE COLORS(ID INTEGER NOT NULL PRIMARY KEY,COLOR CHAR(16))
CREATE MEMORY TABLE CLASSES(ID INTEGER NOT NULL PRIMARY KEY,COMMENT CHAR(40),COLORID INTEGER,CONSTRAINT SYS_FK_11 FOREIGN KEY(COLORID) REFERENCES COLORS(ID))
CREATE MEMORY TABLE ORGANISATION(ID CHAR(8) NOT NULL PRIMARY KEY,COMMENT CHAR(40))
CREATE MEMORY TABLE ADDRESS(ID CHAR(8) NOT NULL PRIMARY KEY,NAME CHAR(32),STREET CHAR(256),CITY CHAR(256),COUNTRY CHAR(256),TELEPHONE CHAR(32),EMAIL CHAR(64),DB_BEGIN DATE,DB_END DATE)
CREATE MEMORY TABLE BANK(ID CHAR(8) NOT NULL PRIMARY KEY,NAME CHAR(32),BLZ CHAR(32),KTNO CHAR(32),DB_BEGIN DATE,DB_END DATE)
CREATE MEMORY TABLE PERSON(ID CHAR(20) NOT NULL PRIMARY KEY,NAME CHAR(32),FIRSTNAME CHAR(32),SHORTNAME CHAR(8),PASSWORD CHAR(8),ORGAID CHAR(8) NOT NULL,BIRTHDAY DATE,COMMENT CHAR(40),CLASSID INTEGER,HOLIDAYS FLOAT,OVERTIME FLOAT DEFAULT 0.0E0,DAYHOURS FLOAT,WEEKDAYS INTEGER,DB_BEGIN DATE,DB_END DATE,ADDRID CHAR(8),BANKID CHAR(8),CONSTRAINT SYS_FK_10 FOREIGN KEY(CLASSID) REFERENCES CLASSES(ID),CONSTRAINT SYS_FK_12 FOREIGN KEY(ORGAID) REFERENCES ORGANISATION(ID),CONSTRAINT SYS_FK_20 FOREIGN KEY(ADDRID) REFERENCES ADDRESS(ID),CONSTRAINT SYS_FK_21 FOREIGN KEY(BANKID) REFERENCES BANK(ID))
CREATE MEMORY TABLE CUSTOMER(ID CHAR(8) NOT NULL PRIMARY KEY,COMMENT CHAR(40),ADDRID CHAR(8),BANKID CHAR(8),CONSTRAINT SYS_FK_22 FOREIGN KEY(ADDRID) REFERENCES ADDRESS(ID),CONSTRAINT SYS_FK_23 FOREIGN KEY(BANKID) REFERENCES BANK(ID))
CREATE MEMORY TABLE PROJECT(ID CHAR(8) NOT NULL PRIMARY KEY,COMMENT CHAR(40),CUSTID CHAR(8),PAY FLOAT,MAXHOURS FLOAT,DB_BEGIN DATE,DB_END DATE,OWNERPROJID CHAR(8) DEFAULT NULL,CONSTRAINT SYS_FK_1 FOREIGN KEY(CUSTID) REFERENCES CUSTOMER(ID),CONSTRAINT SYS_FK_44 FOREIGN KEY(OWNERPROJID) REFERENCES PROJECT(ID))
CREATE MEMORY TABLE PROJECTSTATUS(ID CHAR(20) NOT NULL PRIMARY KEY,COMMENT CHAR(40),COLORID INTEGER,CONSTRAINT SYS_FK_47 FOREIGN KEY(COLORID) REFERENCES COLORS(ID))
CREATE MEMORY TABLE PROJECTDETAILS(ID CHAR(8) NOT NULL PRIMARY KEY,PROJID CHAR(8),COMMENT CHAR(512),STATUS CHAR(8),STATUSDELAY INTEGER,PERSID CHAR(20),FINISHED DATE,CONSTRAINT SYS_FK_43 FOREIGN KEY(PROJID) REFERENCES PROJECT(ID),CONSTRAINT SYS_FK_45 FOREIGN KEY(STATUS) REFERENCES PROJECTSTATUS(ID),CONSTRAINT SYS_FK_46 FOREIGN KEY(PERSID) REFERENCES PERSON(ID))
CREATE MEMORY TABLE TYPES(ID CHAR(4) NOT NULL PRIMARY KEY,COMMENT CHAR(40),DB_BEGIN TIME,DB_END TIME,PAUSE TIME,FREE CHAR(1),CALC FLOAT,COLORID INTEGER,INVISIBLE CHAR(1),CONSTRAINT SYS_FK_15 FOREIGN KEY(COLORID) REFERENCES COLORS(ID))
CREATE MEMORY TABLE HOLIDAY(DAY DATE NOT NULL PRIMARY KEY,COMMENT CHAR(40))
CREATE MEMORY TABLE TIMES(ORGAID CHAR(8) NOT NULL,PERSID CHAR(20) NOT NULL,PROJID CHAR(8) NOT NULL,TYPEID CHAR(8) NOT NULL,COMMENT CHAR(128),DAY DATE NOT NULL,DB_BEGIN TIME NOT NULL,DB_END TIME,PAUSE TIME,CREATION TIMESTAMP DEFAULT NOW,PRIMARY KEY(ORGAID,PERSID,PROJID,TYPEID,DAY,DB_BEGIN),CONSTRAINT SYS_FK_4 FOREIGN KEY(ORGAID) REFERENCES ORGANISATION(ID),CONSTRAINT SYS_FK_5 FOREIGN KEY(PERSID) REFERENCES PERSON(ID),CONSTRAINT SYS_FK_6 FOREIGN KEY(TYPEID) REFERENCES TYPES(ID),CONSTRAINT SYS_FK_7 FOREIGN KEY(PROJID) REFERENCES PROJECT(ID))
CREATE MEMORY TABLE KEYS(ID CHAR(8) NOT NULL PRIMARY KEY,TYPE CHAR(8),COMMENT CHAR(8))
CREATE MEMORY TABLE PROPERTIES(PERSID CHAR(20) NOT NULL,ID INTEGER NOT NULL,KEY CHAR(8) NOT NULL,VALUE CHAR(255),PRIMARY KEY(PERSID,ID,KEY),CONSTRAINT SYS_FK_8 FOREIGN KEY(PERSID) REFERENCES PERSON(ID),CONSTRAINT SYS_FK_9 FOREIGN KEY(KEY) REFERENCES KEYS(ID))
CREATE MEMORY TABLE CHANGES(USER CHAR(64),TABLE CHAR(64),ROWID CHAR(64),ACTION CHAR(128),CHANGED TIMESTAMP NOT NULL PRIMARY KEY)
CREATE MEMORY TABLE PLANCONSTRAINTS(ID CHAR(8) NOT NULL PRIMARY KEY,COMMENT CHAR(128),EXPRESSION CHAR(1024),MODE INTEGER,ERRORMSG CHAR(256))
CREATE MEMORY TABLE PERSONRHYTHM(PERSID CHAR(20) NOT NULL,DAY INTEGER NOT NULL,TYPEID CHAR(8),PRIMARY KEY(PERSID,DAY),CONSTRAINT SYS_FK_13 FOREIGN KEY(PERSID) REFERENCES PERSON(ID),CONSTRAINT SYS_FK_14 FOREIGN KEY(TYPEID) REFERENCES TYPES(ID))
CREATE MEMORY TABLE COMMENTS(NAME CHAR(32) NOT NULL PRIMARY KEY,COMMENT CHAR(1024))
CREATE MEMORY TABLE PERSONHISTORY(ID INTEGER NOT NULL PRIMARY KEY,PERSID CHAR(20) NOT NULL,ORGAID CHAR(8),DB_BEGIN DATE NOT NULL,DB_END DATE NOT NULL,COMMENT CHAR(64),CLASSID INTEGER,HOLIDAYS FLOAT,OVERTIME FLOAT DEFAULT 0.0E0,DAYHOURS FLOAT,WEEKDAYS INTEGER,CONSTRAINT SYS_FK_40 FOREIGN KEY(PERSID) REFERENCES PERSON(ID),CONSTRAINT SYS_FK_41 FOREIGN KEY(CLASSID) REFERENCES CLASSES(ID),CONSTRAINT SYS_FK_42 FOREIGN KEY(ORGAID) REFERENCES ORGANISATION(ID))
