INSERT INTO PLANCONSTRAINTS VALUES('MinClass','At least one Person of Class 5','{C}>=5',1,'Please be sure, you have at least one Person of class 5 in your plan!')
INSERT INTO PLANCONSTRAINTS VALUES('MinClassPerDay','At least one Person of class 5 each day','\u000a\u0009select distinct t2.day from times t2 \u000a\u0009where t2.day >= ''[medBegin(dbdate)]''\u000a\u0009and t2.day <= ''[medEnd(dbdate)]''\u000a\u0009and t2.orgaid like ''[cbbOrganisation]%''\u000a\u0009and day not in (\u000a\u0009\u0009select day from times t3, Person, types\u000a\u0009\u0009where t3.day >= ''[medBegin(dbdate)]''\u000a\u0009\u0009and t3.day <= ''[medEnd(dbdate)]''\u000a\u0009\u0009and t3.orgaid like ''[cbbOrganisation]%''\u000a\u0009\u0009and types.id = t3.typeid\u000a\u0009\u0009and types.free != 1\u000a\u0009\u0009and Person.id = t3.persid\u000a\u0009\u0009and Person.classid >= 5) \u000a\u0009order by day\u0009\u000a',4,'For this days are no Persons of class 5 and higher defined')
INSERT INTO PLANCONSTRAINTS VALUES('MinClassPerDay1','At least one Person of class 2 each day','\u000a\u0009select distinct t2.day from times t2 \u000a\u0009where t2.day >= ''[medBegin(dbdate)]''\u000a\u0009and t2.day <= ''[medEnd(dbdate)]''\u000a\u0009and t2.orgaid like ''[cbbOrganisation]%''\u000a\u0009and day not in (\u000a\u0009\u0009select day from times t3, Person, types\u000a\u0009\u0009where t3.day >= ''[medBegin(dbdate)]''\u000a\u0009\u0009and t3.day <= ''[medEnd(dbdate)]''\u000a\u0009\u0009and t3.orgaid like ''[cbbOrganisation]%''\u000a\u0009\u0009and types.id = t3.typeid\u000a\u0009\u0009and types.free != 1\u000a\u0009\u0009and Person.id = t3.persid\u000a\u0009\u0009and Person.classid = 2) \u000a\u0009order by day\u0009\u000a',4,'For this days are no Persons of class 2 defined')
